{
    "name": "spiral/app-keeper",
    "type": "project",
    "license": "MIT",
    "description": "Spiral Skeleton Application - Keeper",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/app-keeper/issues",
        "source": "https://github.com/spiral/app-keeper"
    },
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "spiral/cycle-bridge": "^1.0",
        "fzaninotto/faker": "^1.9",
        "spiral/framework": "^2.9",
        "spiral/helpers": "^1.0",
        "spiral/keeper": "^0.8",
        "spiral/nyholm-bridge": "^1.0",
        "spiral/roadrunner-bridge": "^1.0",
        "spiral/toolkit": "^v1.1",
        "spiral/writeaway": "^0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv",
            "rr get-binary"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "publish-cmd": "php app.php publish"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
