{
    "name": "spiral/app-keeper",
    "type": "project",
    "license": "MIT",
    "description": "Spiral Skeleton Application - Keeper",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/app-keeper/issues",
        "source": "https://github.com/spiral/app-keeper"
    },
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "ext-json": "*",
        "doctrine/collections": "^1.7",
        "fakerphp/faker": "^1.19",
        "spiral/cycle-bridge": "^2.0",
        "spiral/framework": "^3.4",
        "spiral/helpers": "^1.0",
        "spiral/keeper": "^0.10",
        "spiral/nyholm-bridge": "^1.2",
        "spiral/roadrunner-bridge": "^3.3",
        "spiral/sapi-bridge": "^1.0",
        "spiral/toolkit": "^2.0",
        "spiral/writeaway": "^0.5"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.2",
        "symfony/var-dumper": "^6.0",
        "spiral-packages/ignition-bridge": "^1.0",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5.24"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv",
            "rr get-binary"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        },
        "files": [
            "app/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "publish-cmd": "php app.php publish"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "allow-plugins": {
            "spiral/composer-publish-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
